#include <iostream>
using namespace std;

int main() {
    int N, sum = 0;

    cout << "Enter a positive integer N: ";
    cin >> N;

    // Calculating the sum of first N natural numbers
    for (int i = 1; i <= N; ++i) {
        sum += i;
    }

    cout << "The sum of the first " << N << " natural numbers is: " << sum << endl;

    return 0;


}
