// Increment and decrement values

#include <iostream>
using namespace std;
int main(){
    int x= 5;
    x = x + 100;
    cout<<x<<endl;
    x = x - 90;
    cout<<x<<endl;
    x = x / 3;
    cout<<x<<endl;
}
